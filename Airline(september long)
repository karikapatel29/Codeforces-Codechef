#include <bits/stdc++.h>
using namespace std;

/////////////////////////////////////////////////////////////////
#define testcase                int t;cin>>t;while(t--)
#define ll                      long long int
#define pl                      pair<ll,ll>
#define forl(i,a,b)             for(ll i=a;i<b;i++) 
#define vect                    vector<int>
#define vectl                   vector<ll>
#define vectp                   vector<pl>
#define sortarr(a,n)            sort(a,a+n)
#define ascen(v)                sort(v.begin(),v.end())
#define descen(v)               sort(v.rbegin(),v.rend()
#define fastnfurious            ios::sync_with_stdio(0);cin.tie(0);
#define puba                    push_back    
/////////////////////////////////////////////////////////////////

void kaam()
{
   int a[3],d,e,flag=0;
   forl(i,0,3)
   cin>>a[i];
   cin>>d>>e;
   sortarr(a,3);
   if(a[0]>e)
   {cout<<"NO"<<endl;return;}
   if(a[0]<=e &&a[1]+a[2]<=d)
   flag=1;
   else if(a[1]<=e &&a[0]+a[2]<=d)
   flag=1;
   else if(a[2]<=e &&a[1]+a[0]<=d)
   flag=1;
   if(flag)
   cout<<"YES"<<endl;
   else
   cout<<"NO"<<endl;
   
   
}
int main()
{   
    fastnfurious
  testcase
     kaam();	
	return 0;
}
